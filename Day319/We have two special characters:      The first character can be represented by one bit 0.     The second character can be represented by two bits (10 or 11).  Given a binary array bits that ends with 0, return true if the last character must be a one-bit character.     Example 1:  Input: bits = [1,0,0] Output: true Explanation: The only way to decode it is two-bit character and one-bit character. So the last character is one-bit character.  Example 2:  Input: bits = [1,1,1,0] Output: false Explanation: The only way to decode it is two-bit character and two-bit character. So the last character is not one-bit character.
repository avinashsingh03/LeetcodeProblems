class Solution {
public:
    bool isOneBitCharacter(vector<int>& bits) {
        int size=bits.size();
        if(size==1){
            return true;
        }
        int i=0;
        bool ans=true;
        while(i<size){
            if(bits[i]==1){
                ans=false;
                i+=2;

            }
            else{
                ans=true;
                i+=1;
            }
        }
        return ans;
    }
};
//by self
