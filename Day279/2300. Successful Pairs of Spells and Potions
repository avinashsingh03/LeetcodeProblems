class Solution {
public:
    vector<int> successfulPairs(vector<int>& spells, vector<int>& potions, long long success) {
        int n=spells.size();
        int m=potions.size();
        int start,end;
        vector<int> pairs(n);

        sort(potions.begin(),potions.end());

        for(int i=0;i<n;i++){
            start=0;
            end=m-1;
            while(start<=end){
                int mid= start +(end-start)/2;
                if((long long)potions[mid]*spells[i] >= (success)){
                    end=mid-1;
                }
                else{
                    start=mid+1;
                }
            }
            pairs[i]=m-end-1;
        } 
        return pairs;
    }
};

//by self
