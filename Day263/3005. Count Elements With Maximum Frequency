class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
        vector<int> arr(100,0);
        for(int i=0;i<nums.size();i++){
            arr[nums[i]-1]++;
        }
        int count=0,val;
        val= *max_element(arr.begin(),arr.end());
        for(int i=0;i<100;i++){
            if(arr[i]==val){
                count+=val;
            }
        }
        return count;
    }
};
