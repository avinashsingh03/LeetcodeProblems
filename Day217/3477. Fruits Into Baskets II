class Solution {
public:
    int numOfUnplacedFruits(vector<int>& fruits, vector<int>& baskets) {
        int unplaced=0;
        for(int i=0;i<fruits.size();i++){
            int j=0;
            while(j<baskets.size()){
                if(fruits[i]<=baskets[j]){
                    baskets[j]=-1;
                    break;
                }
                j++;
            }
            if(j==baskets.size()){
                unplaced+=1;
            }
        }
        return unplaced;
    }
};
