class Solution {
public:
    int minNumberOperations(vector<int>& target) {
        int n=target.size();

        int result=target[0];

        for(int i=0;i<n-1;i++){
            if(target[i+1]>target[i]){
                result+=target[i+1]-target[i];
            }
        }
        return result;
        
    }
};
